{
  "name": "rawphp/slicer",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Kaczocha",
      "email": "tom@rawphp.org"
    }
  ],
  "require": {
    "symfony/console": "2.7",
    "symfony/event-dispatcher": "2.*",
    "symfony/filesystem": "2.*",
    "symfony/finder": "2.*",
    "symfony/process": "2.*",
    "seld/phar-utils": "^1.0@dev",
    "seld/jsonlint": "*",
    "guzzlehttp/guzzle": "^6.0@dev",
    "rappasoft/laravel-helpers": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.x-dev",
    "mockery/mockery": "^1.0@dev",
    "theseer/phpdox": "dev-master"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-0": {
      "Slicer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Slicer\\Test": "tests/"
    }
  },
  "bin": [
    "bin/slicer"
  ],
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
